@model Lab3.Modells.GreetingModel
<head>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css">
</head>
<div class="container">
        <div class="jumbotron">
            <h1> @Model.DayMessage</h1>
            <h3> The time is: @Model.Time on @Model.DayOfWeek @Model.Month / @Model.Day / @Model.Year</h3>
            <h3> @Model.DaysLeftNextYears more days until 2018!</h3>
        </div>
        <a asp-action="Create" asp-controller="People" class="btn btn-default">Lab 4 - Add Person</a>   
</div>



